{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "v12.16.1",
    "npm": "v6.14.5"
  },
  "scripts": {
    "start": "node app.js",
    "build": "cd client && npm run build",
    "client": "npm start --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "dev": "concurrently \" nodemon bin/www\" \"cd client && yarn start\""
  },
  "dependencies": {
    "concurrently": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.26",
    "morgan": "~1.9.1"
  }
}
